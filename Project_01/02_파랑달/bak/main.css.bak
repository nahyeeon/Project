@charset "UTF-8";

/* 3e68c3 */

#wrap{width:1200px; margin:0 auto;}

/* header */
header{height:115px; overflow:hidden;}
header img{float:left;}
header ul{float:right; text-align:center; padding-top:31px;}
header ul li{float:left; line-height:145%; border-right:1px solid #ddd; font-size:14px;
padding:0 32px;}
header ul li:last-child{padding-right:0; border:none;}
header ul li b{font-size:17px; letter-spacing:-1px;}

/* visual */
.visual .hot{height:33px; line-height:33px; border:1px solid #ddd; overflow:hidden;}
.visual .hot p{float:left;}
.visual .hot p b{padding-left:24px; padding-right:34px; color:#cc0001;}
.visual .hot ul{float:right;}
.visual .hot ul li{float:left;}

.visual .issue{position:relative; height:428px; width:100%; background:url(../image/main/visual.jpg) no-repeat center center;
margin:27px 0;}
.visual .issue div{position:absolute; left:50px; bottom:75px; font-size:22px; color:#fff;}
.visual .issue div h2{font-weight:400; padding-bottom:10px;}
.visual .issue div h3{font-weight:100; line-height:105%;}

.visual .story{padding-bottom:48px; border-bottom:1px solid #ddd;}
.visual .story ul{overflow:hidden; height:400px; box-sizing:border-box; -webkit-box-sizing:border-box;
-moz-box-sizing:border-box;}
.visual .story ul li{float:left;}

.visual .story .adv{width:450px;}
.visual .story .adv > ul{display:table; box-sizing:border-box; -webkit-box-sizing:border-box;
-moz-box-sizing:border-box;}
.visual .story .adv > ul li{position:relative; display:table-cell; width:200px; height:56px;
padding-top:16px;}
.visual .story .adv > ul li p{position:absolute; left:105px; top:16px; font-size:13px; line-height:110%;
font-weight:500;}
.visual .story .adv > ul li span{display:block; position:absolute; left:105px; bottom:0; font-size:13px; color:#5f5f5f;}
.visual .story .adv > ul li:nth-child(even) p,
.visual .story .adv > ul li:nth-child(even) span,
.visual .story .adv > ul li:nth-child(even){padding-left:13px;}

.visual .story .bluemoon{width:440px; height:400px; position:relative; color:#fff; background:url(../image/main/img1.jpg) no-repeat; margin:0 14px;}
.visual .story .bluemoon > span{position:absolute; left:10px; top:0; font-weight:700; font-size:25px;}
.visual .story .bluemoon > div{position:absolute; bottom:22px; left:140px; text-align:center;}
.visual .story .bluemoon > div img{position:absolute; bottom:22px; left:-45px; padding-right:5px;}
.visual .story .bluemoon > div b{font-size:32px;}
.visual .story .bluemoon > div p{font-size:20px; font-family:'Nanum Pen Script', cursive;}

.visual .story .chilling{width:280px;}
.visual .story .chilling > ul{position:relative;}
.visual .story .chilling > ul li{text-align:center; width:280px; background-color:#e9e9e9;}
.visual .story .chilling > ul li:first-child a{background:#3f68c4; color:#fff;}
.visual .story .chilling > ul li:first-child a span{color:#3f68c4;}
.visual .story .chilling > ul li:last-child{margin-top:14px;}
.visual .story .chilling > ul li a{display:block; padding:34px 0;}
.visual .story .chilling > ul li h2{font-weight:400;}
.visual .story .chilling > ul li p{padding:15px 0; font-size:16px; line-height:110%;}
.visual .story .chilling > ul li span{padding:5px 10px; border-radius:35px; -webkit-border-radius:35px; -moz-border-radius:35px; background:#fff; border:1px solid #ddd;
font-weight:400;}

/* content */
.new h3,
.notice h3,
.product h3,
.sns h3{position:relative; font-size:24px; font-weight:400; color:#3f7ec3; padding-top:28px; padding-bottom:15px; margin-bottom:20px;}
.new h3:after,
.notice h3:after,
.product h3:after,
.sns h3:after{position:absolute; left:0; bottom:0; content:""; background-color:#000; height:1px; width:26px;}
.content h3 > span{position:absolute; right:32px; top:10px; font-size:50px; font-weight:100;}

/* news */
.news{overflow:hidden; border-bottom:1px solid #ddd;}
.news .new{float:left; padding-top:10px; width:670px; border-right:1px solid #ddd; box-sizing:border-box; -webkit-box-sizing:border-box;
-moz-box-sizing:border-box; }
.news .notice{float:right; width:500px; padding-top:10px;}
.news ul li > p{padding-top:14px;}
.news ul li > p b{font-size:16px;}
.news ul li > p span{font-size:13px; color:#bdbdbd;}
/* new */
.content .news .new ul{font-size:0; padding-bottom:43px;}
.content .news .new ul li{display:inline-block; font-size:14px; padding-right:14px;}

/* notice */
.notice{padding-left:30px; height:450px;}
.notice ul li:first-child{border-bottom:1px solid #ddd; padding-top:0;}
.notice ul li{padding:30px 0; padding-right:0;}
.notice ul li p{font-size:15px; line-height:120%;}
.notice ul li p b{font-size:18px;}

/* product */
.product{float:left; width:860px; height:715px; padding-bottom:50px; padding-left:30px;
border-bottom: 1px solid #ddd; background: #EFF1F0;}
.product ul li{display:table;}
.product ul li:nth-child(2){padding:37px 0;}
.product ul li > p,
.product ul li > div{display:table-cell; vertical-align:middle;}
.product ul li > div p{margin-left:15px; font-size:13px; line-height:145%;}
.product ul li > div p:first-child{font-size:20px; font-weight:400;}
.product ul li > div p:nth-child(2){font-size:15px;}
.product ul li > div p:last-child{color:#5f5f5f;}

/* sns */
.sns{float:right; width:275px; height:715px; padding-left:32px; padding-bottom:50px; border-bottom: 1px solid #ddd;}
.sns ul li p:first-child{border:1px solid #ddd; font-size:14px; padding:20px 10px;}
.sns ul li p:nth-child(2){padding:20px 0;}
.sns ul li{position:relative;}
.sns ul li p span{position:absolute; top:157px; left:60px; font-size:15.5px;}

/* footer */
footer{width:1200px; margin:0 auto; position: relative;}
footer > div p{float:left; padding-top:60px; padding-bottom:133px;}
footer > div p span{position: absolute; color:#929292; padding-left:20px;}
footer > div ul{float:right; font-size:0; padding-top:60px; padding-bottom:133px;}
footer > div ul li{display:inline-block; padding-left:5px;}