@charset "utf-8";

.inner{width:1300px; margin:0 auto;}
.content{width:100%;}

/* header */
#mobile_menu{display:none;}
header .head{height:110px; overflow:hidden; text-align:center;}
header .head .inner{position:relative;}
header h1{float:left; line-height:110px; padding-left:20px; padding-top:17px;}
header .map{float:right; margin-right:337px; font-size:0; line-height:110px;}
header .search{position:absolute; right:0; line-height:110px;}

header .map li{display:inline-block; font-size:13px; }
header .map li a{color:#aeaeae; padding:0 5px;}
header .map li:nth-child(2n){color:#ddd;}
header .map li:last-child{padding-right:29px;}

header .search input{position:relative; width:322px; height:41px; border:3px solid #d79fa2; border-radius:23px;
-webkit-border-radius:23px; -moz-border-radius:23px; text-indent:1.5em; font-size:15px; }
header .search input::placeholder{color:#d79fa2; font-weight:bold;}
header .search:after{position:absolute; right:50px; top:42%; width:1px; height:20px; content:""; background:#d79fa2;}

/* visual */
.visual{overflow:hidden; background:#dbc9bd; width:100%; height:890px;}
.visual img{float:right;}
.visual .inner{position:relative;}
.vis{position:absolute; left:0; top:200px; flex-direction:column;}
.vis h2{font-size:52px; font-weight:300; letter-spacing:-3px; color:#fff; line-height:110%;
margin-bottom:30px;}
.vis h2 > b{font-weight:400;}
.vis h2 > p{color:#000; font-weight:bold;}
.vis > p{font-size:15px; color:#79716f; line-height:130%; letter-spacing:-0.5px;}
.vis > p:nth-child(4){color:#fff; font-weight:400; padding-bottom:20px;}
.vis > span a{font-size:16px; font-weight:400; background:#b44c55; color:#fff;
padding:20px 64px; letter-spacing:-1px;}
.visual > span{display:none;}

/* content */
.content h3{font-size:32px; font-weight:400; letter-spacing:-2px;}
.story,
.campaign,
.mov,
.news,
.support{padding-top:80px; padding-bottom:94px; text-align:center;}
.story div h2 img{width:245px; height:52px;}
.story .st_list{margin-top:72px; padding-bottom:94px; border-bottom:1px solid #ddd;}
.story .st_list ul{font-size:0;}
.story .st_list ul li{display:inline-block; width:calc(100% / 4 - 15px); height:410px; font-size:13px;
vertical-align:top; border:2px solid #aeaeb8; text-align:left;
box-sizing:border-box; -webkit-box-sizing:border-box; -moz-box-sizing:border-box; 
margin-right:15px; color:#959595; padding:12px;}
.story .st_list ul li:last-child{margin-right:0;}
.story .st_list ul li,
.story .st_list ul li p img{border-radius:15px; -webkit-border-radius:15px; -moz-border-radius:15px;}
.story .st_list ul li:nth-child(odd){border:2px solid #cc828f;}
.story .st_list ul li:nth-child(even) > div p:first-child{color:#a1a1a9;}
.story .st_list ul li > div{margin:19px 15px;}
.story .st_list ul li > div p:first-child{font-size:15px; font-weight:500; color:#b9455a;}
.story .st_list ul li > div p b{display:block; color:#565656; font-size:18px; line-height:120%; margin:10px 0;}

/* law */
.law{position:relative;}
.law .inner{border-bottom:1px solid #ddd; overflow:hidden; height:836px;}
.law .inner > span{display:block; position:absolute; left:0; top:20%; }
.law .inner > span img{width:692px; height:425px; border-top-right-radius:250px; -webkit-border-top-right-radius:250px;
-moz-border-top-right-radius:250px; border-bottom-right-radius:250px; -webkit-border-bottom-right-radius:250px;
-moz-border-bottom-right-radius:250px;}
.law .inner > div{position:absolute; right:14%; top:20%; width:755px; height:425px;}
.law .inner > div p{font-size:13px; color:#a8a8a8; line-height:145%; letter-spacing:-1px;}
.law .inner > div p:first-child{font-size:22px; color:#5f5f5f; letter-spacing:-2px; }
.law .inner > div p:nth-child(2){color:#000; font-size:52px; font-weight:300; letter-spacing:-4px;
line-height:120%; padding:20px 0;}
.law .inner > div p:nth-child(3){padding-bottom:30px;}
.law .inner > div p a{display:inline-block; font-size:18px; padding:25px 65px; color:#fff; font-weight:400; 
border-radius:50px; -webkit-border-radius:50px; -moz-border-radius:50px; background:#ba5b63; margin-right:10px;}
.law .inner > div p a:last-child{background:#848693;}

/* campaign */
.campaign{padding-bottom:0;}
.campaign div:nth-child(1) p{padding-top:10px; margin-bottom:65px; letter-spacing:-1px; color:#9e9e9e;}
.campaign .inner{text-align:center; vertical-align:middle; color:#fff; width:100%; height:682px;
background:url(../image/main/bg.jpg) no-repeat center center; position:relative;}
.campaign .inner p{position:absolute; left:34%; top:55%; font-weight:400; color:#d1bdb6; font-size:28px; line-height:130%;}
.campaign span a{display:inline-block; font-size:22px; color:#000; border:1px solid #000; padding:22px 94px; margin-top:50px;}

/* mov */
.mov{background:#ededed;}
.mov p{padding-top:10px; margin-bottom:65px; letter-spacing:-1px; color:#9e9e9e;}
.mov ul{font-size:0;}
.mov ul li{position:relative; display:inline-block; font-size:17px; margin-right:95px;}
.mov ul li:last-child{margin:0;}
.mov ul li p{color:#5f5f5f; padding-top:30px;}
.mov ul li:nth-child(3) p{width:205px; height:137px; padding-top:63px; font-size:20px; font-weight:400; line-height:120%; position:absolute; left:60px; top:66px; color:#fff; background:rgba(0,0,0,.7); border-radius:50%;}
.mov ul li:nth-child(3) p a{display:block; color:#fff; font-weight:200; font-size:15px; margin-top:15px;}

/* news */
.news h3{padding-bottom:92px;}
.news .inner{border-bottom:1px solid #ddd; padding-bottom:92px; height:787px;}
.news .inner .left{float:left; width:49.9%; border-right:1px solid #ddd;}
.news .inner .right{float:right; width:49.9%; }




